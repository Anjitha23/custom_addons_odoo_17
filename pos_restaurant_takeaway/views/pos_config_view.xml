<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- form view-->
    <record id="pos_takeaway_dine_in_conf_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id"
               ref="point_of_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@id='pos_inventory_section']" position="after">
                <block title="Other Features" id="pos_takeaway">
                    <setting
                            title="TakeAway,Dine-in"
                            string="Pos TakeAway"
                            help="TakeAway,Dine-in on Restaurant">
                        <field name="dine_in"
                               required="pos_config_id"/>
                    </setting>
                    <setting string="Generate Token"
                             help="This Token number starts from 1">
                    <field name="generate_token"/>
                    </setting>
                </block>
            </xpath>
        </field>
    </record>
</odoo>


